<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.example.org/UUID/" targetNamespace="http://www.example.org/UUID/">

    <element name="uuid" type="tns:UUID"></element>
    
    <simpleType name="UUID">
    	<restriction base="string">
    		<pattern
    			value="[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}">
    		</pattern>
    	</restriction>
    </simpleType>
</schema>